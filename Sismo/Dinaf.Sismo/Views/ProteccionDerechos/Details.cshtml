                    <ul class="nav nav-tabs nav-tabs-bordered" id="profiles" role="tablist">
                        @for (int i = 0; i < Model.Personas.Count; i++)
                        {
                            <li class="nav-item" role="presentation">
                                <button class='nav-link @(i == 0 ? "active" : "")' id="@Model.Personas[i].Id" data-bs-toggle="tab" data-bs-target="#content_@Model.Personas[i].Id" type="button" role="tab" aria-controls="content_@Model.Personas[i].Id" aria-selected="true">@Model.Personas[i].NombreCorto</button>
                            </li>
                        }
                    </ul>

                    <div class="tab-content pt-2" id="profilesContent">
                        @for (int i = 0; i < Model.Personas.Count; i++)
                        {
                            <div class='tab-pane fade @(i == 0 ? "show active" : "")' id="content_@Model.Personas[i].Id" role="tabpanel" aria-labelledby="@Model.Personas[i].Id">
                                <div class="card-body profile-card pt-4 d-flex flex-column align-items-center">
                                    <img src="/img/userrrr.jfif" alt="Profile" class="rounded-circle">
                                    <h2>@Model.Personas[i].NombreCorto</h2>
                                    <h3>@Model.Personas[i].Ocupacion</h3>
                                    <div class="social-links mt-2">
                                        <div class="row">
                                            <div class="col-lg-9 col-md-4 label "><b>Nombre Completo</b></div>
                                            <div class="col-lg-12 col-md-8">@Model.Personas[i].NombreCompleto</div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-9 col-md-4 label "><b>identidad</b></div>
                                            <div class="col-lg-12 col-md-8">0801-1977-07557</div>
                                        </div>
