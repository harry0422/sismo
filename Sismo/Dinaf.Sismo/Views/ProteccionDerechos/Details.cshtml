                                            <div class="col-lg-12 col-md-8">@Model.Personas[i].FechaNacimiento</div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-9 col-md-4 label "><b>Edad</b></div>
                                            <div class="col-lg-12 col-md-8">@Model.Personas[i].Edad Años</div>
                                        </div>
                                        <div class="row">
                                            <div class="col-lg-9 col-md-4 label "><b>Nacionalidad</b></div>
                                            <div class="col-lg-12 col-md-8">@Model.Personas[i].Nacionalidad</div>
                                        </div>
